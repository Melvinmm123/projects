    "from tensorflow.keras import losses, optimizers\n",
    "\n",
    "# fix random seed for reproducibility\n",
    "seed = 7\n",
    "np.random.seed(seed)\n",
    "\n",
    "\n",
    "h5f = h5py.File('SVHN_single_grey1.h5', 'r')\n",
    "\n",
